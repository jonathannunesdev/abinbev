{
  "name": "abi-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=DEV ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify -r tsconfig-paths/register src/main.ts",
    "start:debug": "NODE_ENV=DEV ts-node-dev --respawn --watch --debug src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint --config eslint.config.mjs \"src/**/*.{js,ts,tsx}\" \"test/**/*.{js,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.25",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.5.0",
    "prettier": "^3.5.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.36.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@config$": "<rootDir>/config",
      "^@infra/(.*)$": "<rootDir>/infra/$1",
      "^@controllers$": "<rootDir>/presentation/controllers/index",
      "^@abstract/(.*)$": "<rootDir>/core/abstract/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@core$": "<rootDir>/core/abstract/gateway/index",
      "^@usecases$": "<rootDir>/domain/usecase/index",
      "^@repository$": "<rootDir>/infra/repository/index",
      "^@entity$": "<rootDir>/domain/entity/index",
      "^@domain/(.*)$": "<rootDir>/domain/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1",
      "^@database$": "<rootDir>/infra/database/index",
      "^@middleware$": "<rootDir>/infra/http/middleware/index",
      "^@gateway$": "<rootDir>/infra/gateway/jwt-gateway",
      "^@gateways$": "<rootDir>/infra/gateway/index",
      "^@nestjs$": "<rootDir>/infra/http/nestjs/index",
      "^@entities$": "<rootDir>/infra/orm/entities/index",
      "^@orm/(.*)$": "<rootDir>/infra/orm/$1"
    }
  }
}
